{
  "name": "holdemheroes",
  "version": "0.0.1",
  "description": "Holdem Heroes NFT & Texas Holdem Game contracts",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holdemheroes/contracts.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holdemheroes/contracts/issues"
  },
  "homepage": "https://github.com/holdemheroes/contracts#readme",
  "scripts": {
    "build": "truffle compile",
    "test": "truffle test",
    "contract-size": "npx truffle run contract-size",
    "coverage": "truffle run coverage --solcoverjs .solcover.js",
    "abis": "node scripts/utils/write_abis.js",
    "abigen": "truffle run abigen TexasHoldemV1 HoldemHeroes",
    "ganache": "yarn run ganache-cli --deterministic",
    "migrate:cards:vordev": "truffle migrate -f 1 --to 1 --network vordev",
    "migrate:cards:ganache": "truffle migrate -f 1 --to 1 --network develop",
    "migrate:cards:rinkeby": "truffle migrate -f 1 --to 1 --network rinkeby",
    "migrate:cards:mainnet": "truffle migrate -f 1 --to 1 --network mainnet",
    "migrate:cards:polygon:mumbai": "truffle migrate -f 1 --to 1 --network polygon_mumbai",
    "migrate:heh:vordev": "truffle migrate -f 2 --to 2 --network vordev",
    "migrate:heh:ganache": "truffle migrate -f 2 --to 2 --network develop",
    "migrate:heh:rinkeby": "truffle migrate -f 2 --to 2 --network rinkeby",
    "migrate:heh:mainnet": "truffle migrate -f 2 --to 2 --network mainnet",
    "migrate:heh:polygon:mumbai": "truffle migrate -f 5 --to 5 --network polygon_mumbai",
    "migrate:phe:vordev": "truffle migrate -f 3 --to 3 --network vordev",
    "migrate:phe:ganache": "truffle migrate -f 3 --to 3 --network develop",
    "migrate:phe:rinkeby": "truffle migrate -f 3 --to 3 --network rinkeby",
    "migrate:phe:mainnet": "truffle migrate -f 3 --to 3 --network mainnet",
    "migrate:phe:polygon:mumbai": "truffle migrate -f 3 --to 3 --network polygon_mumbai",
    "migrate:th:vordev": "truffle migrate -f 4 --to 4 --network vordev",
    "migrate:th:ganache": "truffle migrate -f 4 --to 4 --network develop",
    "migrate:th:rinkeby": "truffle migrate -f 4 --to 4 --network rinkeby",
    "migrate:th:mainnet": "truffle migrate -f 4 --to 4 --network mainnet",
    "migrate:th:polygon:mumbai": "truffle migrate -f 4 --to 4 --network polygon_mumbai",
    "verify:heh:rinkeby": "truffle run verify HoldemHeroes --network rinkeby",
    "verify:heh:mainnet": "truffle run verify HoldemHeroes --network mainnet",
    "lint:sol:fix": "solium -d contracts/ --fix",
    "lint:sol": "solium -d contracts/"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.2",
    "@truffle/hdwallet-provider": "^1.5.0",
    "@unification-com/xfund-vor": "^0.2.6",
    "dotenv": "^10.0.0",
    "ethereumjs-util": "^7.1.1",
    "ethers": "^5.4.7",
    "minimist": "^1.2.5",
    "prb-math": "^2.4.3",
    "web3": "^1.5.3",
    "web3-providers-ws": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@chainsafe/truffle-plugin-abigen": "^0.0.2",
    "@openzeppelin/test-helpers": "^0.5.13",
    "@pinata/sdk": "^1.1.23",
    "args": "^5.0.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "csv-writer": "^1.6.0",
    "eth-gas-reporter": "^0.2.22",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "safe-buffer": "^5.2.1",
    "solidity-coverage": "^0.7.17",
    "solium": "^1.2.5",
    "truffle": "^5.4.11",
    "truffle-contract-size": "^2.0.1",
    "truffle-plugin-verify": "^0.5.15"
  }
}
